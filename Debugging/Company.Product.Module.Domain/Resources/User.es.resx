  <data name="CreateUserEmailBody" xml:space="preserve">
    <value>&lt;html&gt;
&lt;head&gt;
    &lt;style&gt;
        body {
            text-align: center;
            font-family: monospace;
        }

        .app-logo {
            margin: 15px;
        }

        .app-text {
            margin: 12px;
            color: rgb(15, 20, 25);
            font-weight: 600;
            font-size: 14px;
        }

        .app-text-small {
            margin: 10px;
            color: rgb(15, 20, 25);
            font-weight: 600;
            font-size: 13px;
        }

        .app-credentials &gt; p {
            margin: 8px;
            color: rgb(15, 20, 25);
            font-weight: 500;
            display: inline;
            font-size: 12px;
        }

        .app-credentials label {
            margin: 8px;
            color: rgb(29, 155, 240);
            font-weight: 100;
            font-size: 12px;
        }
    &lt;/style&gt;
&lt;/head&gt;
&lt;body&gt;
    &lt;div class="app-logo"&gt;
        &lt;img width="100" src="https://www.google.com/images/branding/googlelogo/2x/googlelogo_color_272x92dp.png" /&gt;
    &lt;/div&gt;
    &lt;div class="app-text"&gt;
        Has sido registrado como usuario en al aplicación {APLICACION}
    &lt;/div&gt;
    &lt;div class="app-text-small"&gt;
        Tus credenciales son:
    &lt;/div&gt;
    &lt;div class="app-credentials"&gt;
        &lt;p&gt;Usuario:&lt;/p&gt;&lt;label&gt;{USER}&lt;/label&gt;
    &lt;/div&gt;
    &lt;div class="app-credentials"&gt;
        &lt;p&gt;Contraseña:&lt;/p&gt;&lt;label&gt;{PASSWORD}&lt;/label&gt;
    &lt;/div&gt;
&lt;/body&gt;
&lt;/html&gt;</value>
